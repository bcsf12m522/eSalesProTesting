<body id="body-index">

    <div id="sticky-footer-wrap">


        @*-----------------Top Navigation Start---------------*@

        @{
            Html.RenderPartial("Top_Navigation_View");
        }

        @*-----------------Top Navigation End---------------*@

        <div class="container-fluid main-container">

            @*-----------------Side Navigation Start---------------*@
            <div class="row">
                @{
                    Html.RenderPartial("Side_Navigation_View");
                }
                @*-----------------Side Navigation End---------------*@

                <div class="right_side_custom_style">
                    <div class="bggg-img">

                        <!-- Button trigger modal -->
                        <button class="btn btn-primary btn-lg" data-toggle="modal" data-target="#changeTime">Show Modal</button>
                        <!-- Modal -->
                        <div class="modal fade" id="changeTime" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                            <div class="modal-dialog">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                                        <h4 class="modal-title" id="myModalLabel">Change time</h4>
                                    </div>
                                    <div class="modal-body">
                                        <form>
                                            <table>
                                                <tr>
                                                    <td class="description">
                                                        <span class="descriptionText1">description</span>
                                                    </td>
                                                    <td style="width:50px;">
                                                        <span class="timer pull-right btn btn-primary">10 seconds</span>
                                                    </td>
                                                </tr>
                                            </table>
                                        </form>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                        <button type="button" class="btn btn-primary">Save changes</button>
                                    </div>
                                </div>
                            </div>
                        </div>


                    </div>
                </div>
                @*-----------------Main Container Start---------------*@
                @*-----------------Main Container Start---------------*@


                @*-----------------Main Container Ends---------------*@
            </div>
            <div id="sticky-footer-push"></div>
        </div>
    </div>

    <footer class="black">

        @{
            Html.RenderPartial("Footer_View");
        }
    </footer>


    <script>
        $("[data-toggle='modal']").click(function (e) {
            e.preventDefault();
            e.stopPropagation();
        });

        $("[data-toggle='modal']").on("contextmenu", function (e) {
            e.preventDefault();
            $("#changeTime").modal("show");
        })
    </script>





</body>